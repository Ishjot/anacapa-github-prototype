<p>
  <strong>Dept:</strong>
  <%= @course.dept %>
</p>

<p>
  <strong>Num:</strong>
  <%= @course.num %>
</p>

<p>
  <strong>Desc:</strong>
  <%= @course.desc %>
</p>

<%= link_to 'Edit', edit_course_path(@course) %> |
<%= link_to 'Back', courses_path %>

<hr>
<p>Students:</p>
<ul>
<% @course.students.each do |student| %>
    <li><%= link_to student.first_name << " " << student.last_name << " (" << student.email << ")", student %></li>
<% end %>
</ul>
